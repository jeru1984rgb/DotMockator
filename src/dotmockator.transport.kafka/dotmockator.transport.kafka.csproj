<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyName>DotMockator.Transport.Kafka</AssemblyName>
        <RootNamespace>DotMockator.Transport.Kafka</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="KafkaFlow" Version="2.2.4" />
      <PackageReference Include="KafkaFlow.Microsoft.DependencyInjection" Version="2.2.4" />
      <PackageReference Include="KafkaFlow.Serializer" Version="2.2.4" />
      <PackageReference Include="KafkaFlow.Serializer.JsonCore" Version="2.2.4" />
      <PackageReference Include="KafkaFlow.Serializer.ProtobufNet" Version="2.2.4" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\dotmockator.core\dotmockator.core.csproj" />
    </ItemGroup>

</Project>
